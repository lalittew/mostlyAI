{
    "env":{
        "baseUrl": "https://mostly.ai/"
    },
    "testFiles": ["**/*.feature"],
    "chromeWebSecurity": false,
    "viewportWidth": 2000,
    "viewportHeight": 800,
    "reporter": "cypress-mochawesome-reporter",
    "video": true,
    "retries": 0,
    "reporterOptions": {
        "embeddedScreenshots": true,
        "charts": true,
        "reporterPageTitle": "Cypress Inline Report",
        "inlineAssets": true
    }
}
